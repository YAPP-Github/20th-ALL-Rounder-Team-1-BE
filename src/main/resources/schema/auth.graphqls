input LoginInput {
	email: String!
	password: String!
}

type LoginResponse {
	accessToken: String!
	refreshToken: String!
}

type ReissueAccessTokenResponse {
	accessToken: String!
}

input ValidAuthKeyInput {
	authKey: String!
	email: String!
}

input SignUpInput {
	email: String!
	password: String!
	nickName: String!
	jobs: [String!]
	interests: [String!]
	marketingAgreed: Boolean
}
